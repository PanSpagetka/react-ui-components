{
  "name": "@manageiq/react-ui-components",
  "version": "0.8.0",
  "description": "React based ui components for ManageIQ project.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config scripts/webpack.config.js --watch",
    "build": "webpack --config scripts/webpack.config.js",
    "vendor": "webpack --config scripts/webpack.config.js --env.build",
    "server": "webpack-dev-server --config scripts/webpack.config.js --env.server",
    "test": "jest",
    "doc": "react-docgen",
    "storybook": "start-storybook -c scripts/storybook -p 6006",
    "build-storybook": "build-storybook -c scripts/storybook -o .out",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.8.1"
  },
  "files": [
    "dist/",
    "!dist/vendor.js",
    "!dist/vendor.css"
  ],
  "keywords": [
    "react",
    "manageiq",
    "components",
    "ui"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*",
      "!src/**/vendor.js"
    ],
    "setupFiles": [
      "<rootDir>/scripts/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-info": "^3.3.13",
    "@storybook/addon-links": "^3.3.13",
    "@storybook/addon-options": "^3.3.13",
    "@storybook/addon-storysource": "^3.4.4",
    "@storybook/addons": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.1.1",
    "copy-webpack-plugin": "^4.3.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^3.0.2", 
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "react-router-dom": "^4.2.2",
    "redux-mock-store": "^1.5.1",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "final-form": "^4.7.3",
    "patternfly-react": "^2.5.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "react-final-form": "^3.5.1",
    "react-redux": "^5.0.6",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-form-validators": "^2.7.0",
    "redux-mock-store": "^1.5.1"
  }
}
